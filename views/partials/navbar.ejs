<header id="navbar-banner" class="user-selectnone">



<div class="mobile-nav">
    <div class="mob-item pt-3">
        <span id="header-nav">
            <%for(let page in pageData){%>
                <%if (pageData[page].pageType === 'homePage'){%>
                    <a href="<%=pageData[page].value%>"><%=pageData[page].title%></a>
                <%}%>
            <%}%>
        </span>
        <ul class="d-flex pt-4 flex-column align-items-center list-unstyled">
            <div id="account-option">
                <%for(let page in pageData){%>
                    <%if (pageData[page].pageType === 'accountPages'){%>
                        <li class="nav-link-mobile">
                            <a href="<%=pageData[page].value%>"><%=pageData[page].title%></a>
                        </li>
                    <%}%>
                <%}%>
            </div>
    
            <div>
                <%for(let page in pageData){%>
                    <%if (pageData[page].pageType === 'publicPages'){%>
                        <li class="nav-link-mobile">
                            <a href="<%=pageData[page].value%>"><%=pageData[page].title%></a>
                        </li>
                    <%}%>
                <%}%>
            </div>
        </ul>
    </div>

</div>
<ul class="desktop-nav list-unstyled d-flex flex-row justify-content-between align-items-center mx-3 mx-md-4 mb-0 p-2 p-lg-4">
    <div class="title-nav d-flex flex-row justify-content-between align-items-center">
        <li><span id="header-nav">
            <%for(let page in pageData){%>
                <%if (pageData[page].pageType === 'homePage'){%>
                    <a href="<%=pageData[page].value%>"><%=pageData[page].title%></a>
                <%}%>
            <%}%>
        </span></li>
    </div>
    <div class="title-nav d-flex flex-row align-items-center d-none d-lg-flex">
        <div class="d-flex flex-row justify-content-between align-items-center">
            <%for(let page in pageData){%>
                <%if (pageData[page].pageType === 'publicPages'){%>
                    <li class="nav-link">
                        <a href="<%=pageData[page].value%>"><%=pageData[page].title%></a>
                    </li>
                <%}%>
            <%}%>
        </div>
        <div class="d-flex fle-row justify-content-between align-items-center">
            <%for(let page in pageData){%>
                <%if (pageData[page].pageType === 'accountPages'){%>
                    <%if (pageData[page].value === 'reserve'){ %>
                        <li class="nav-link">
                            <button id="sign-up-btn"><a href="<%=pageData[page].value%>"><%=pageData[page].title%></a></button>

                        </li>
                    <%} else{ %>
                        <li class="nav-link">
                            <a href="<%=pageData[page].value%>"><%=pageData[page].title%></a>
                        </li>
                    <%}%>
                <%}%>
            <%}%>

        </div>
    </div>
    <button class="burger px-4 d-block d-lg-none">
        <i class="fa-solid fa-bars"></i>
    </button>
</ul>

</header>